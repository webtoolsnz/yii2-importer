{
  "name": "webtoolsnz/yii1-importer",
  "description": "Yii2 CSV ActiveRecord importer",
  "keywords": [
    "yii2",
    "import",
    "csv"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "homepage": "https://github.com/webtoolsnz/yii2-importer",
  "authors": [
    {
      "name": "Byron Adams",
      "email": "byron@webtools.co.nz",
      "role": "Developer"
    },
    {
      "name": "Webtools Ltd",
      "email": "support@webtools.co.nz",
      "homepage": "http://www.webtools.co.nz",
      "role": "Developer"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.38",
    "league/csv": "^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "webtoolsnz\\importer\\": "src"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
